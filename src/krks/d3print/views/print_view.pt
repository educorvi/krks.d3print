<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="krks.d3print"
      metal:use-macro="context/main_template/macros/master">
<body>
  <metal:block fill-slot="content-core">

    <div class="Container">
      <div class="col-12 col-md-6">
        <div class="card" style="width: 58rem;">
          <img tal:attributes="src string:${context/absolute_url}/@@images/vorschaubild" class="card-img-top" alt="...">
          <div class="card-body">
        </div>
      </div>
    </div>

    <div class="Container">
      <div class="row">

        <div class="col-12 col-md-6">
          <button type="button" class="btn btn-primary">
            <a tal:attributes="href string:${context/absolute_url}/@@download/printmodell/${context/printmodell/filename}" class="btn btn-primary">Herunterladen</a></button>

          <button type="button" class="btn btn-success">
            <a class="btn btn-success" href="http://192.168.86.56:5001/" role="button">Drucken</a></button>

          <button type="button" class="btn btn-danger">
            <a class="btn btn-danger" href="http://0.0.0.0:8080/Test.Plone/3d-modelle" role="button">Abrechen</a></button>
        </div>
      </div>
    </div>

  </metal:block>
</body>
</html>
