<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="krks.d3print"
      metal:use-macro="context/main_template/macros/master">
<body>
  <metal:block fill-slot="content-core">
    <div tal:define="tempdata view/connect_printer" class="container">
      <div class="col-12 col-md-6">
        <div class="row">
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Eigenschagten</th>
                <th scope="col">Aktuell</th>
                <th scope="col">Ziel</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">3</th>
                <td>Status</td>
                <td>Keine Funktion</td>
                <td>Keine Funktion</td>
              </tr>
              <tr>
                <th scope="row">2</th>
                <td>Tooltemperatur</td>
                <td tal:content="tempdata/tooltemp"></td>
                <td>Keine Funktion</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>Betttemperatur</td>
                <td tal:content="tempdata/bedtemp"></td>
                <td>Keine Funktion</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="col-12 col-md-6">
          <table class="table table-striped">
            <thead>
              <tr>
              <th scope="col">#</th>
                <th scope="col">Objektname</th>
                <th scope="col">Bild</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">1</th>
                <td>N/A</td>
                <td>N/A</td>
              </tr>
              <tr>
                <th scope="row">2</th>
                <td>N/A</td>
                <td>N/A</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>N/A</td>
                <td>N/A</td>
              </tr>
              <tr>
                <th scope="row">4</th>
                <td>N/A</td>
                <td>N/A</td>
              </tr>
            </tbody>
          </table>
        </div>
      <div class="col-12 col-md-6">
        <div class="row">
          <a href="" tal:attributes="href view/aktualisieren" type="button" role="button" class="btn btn-primary">Aktualisieren</a>
        </div>
      </div>
    </div>
  </div>
</metal:block>
</body>
</html>
