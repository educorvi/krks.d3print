<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="krks.d3print"
      metal:use-macro="context/main_template/macros/master">
<body>
  <metal:block fill-slot="content-core">
    <h2 i18n:translate="">Sample View</h2>

      <div tal:define="tempdata view/connect_printer" class="container">
        <div class="col-12 col-md-6">
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Merkmal</th>
                <th scope="col">Ergebnis</th>
                <th scope="col">Handle</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">1</th>
                <td>Tooltemperatur</td>
                <td tal:content="tempdata/tooltemp"></td>
                <td>@mdo</td>
              </tr>
              <tr>
                <th scope="row">2</th>
                <td>Betttemperatur</td>
                <td tal:content="tempdata/bedtemp"></td>
                <td>@fat</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>Larry</td>
                <td>the Bird</td>
                <td>@twitter</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>Larry</td>
                <td>the Bird</td>
                <td>Leerfeld</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>

      <div class="container">
        <div class="row">
        <div class="col-12 col-md-6">
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>
                <th scope="col">Handle</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">1</th>
                <td>Mark</td>
                <td>Otto</td>
                <td>@mdo</td>
              </tr>
              <tr>
                <th scope="row">2</th>
                <td>Jacob</td>
                <td>Thornton</td>
                <td>@fat</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>Larry</td>
                <td>the Bird</td>
                <td>@twitter</td>
              </tr>
              <tr>
                <th scope="row">3</th>
                <td>Larry</td>
                <td>the Bird</td>
                <td>Leerfeld</td>
              </tr>
            </tbody>
          </table>
        </div>
        <div class="col-12 col-md-6">
          <button type="button" class="btn btn-danger">Danger</button>
        </div>
      </div>
      </div>
  </metal:block>
</body>
</html>
